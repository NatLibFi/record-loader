{
    "type": "object",
    "properties": {
	"logging": {
	    "type": "object",
	    "properties": {
		"level": {
		    "type": "string",
		    "enum": [
			"trace",
			"debug",
			"info",
			"warn",
			"error"
		    ]
		}
	    },
	    "default": {
		"level": "warn"
	    }
	},
	"processing": {
	    "type": "object",
	    "properties": {
		"target": {
		    "type": "string",
		    "enum": [
			"filter",
			"preprocess",
			"match",
			"merge",
			"load"
		    ],
		    "default": "load"
		},
		"findRelatedRecords": {
		    "type": "boolean",
		    "default": true
		},
		"resultsLevel": {
		    "description": "Amount of data returned after processing is complete. With default level 'total' only total amounts are returned. Level 'record' returns also record-specific metadata about processing. Level 'debug' return the final representation at the target processing step of each record along with metadata",
		    "type": "string",
		    "enum": [
			"total",
			"record",
			"debug"
		    ],
		    "default": "total"
		},
		"transaction": {
		    "type": "object",
		    "properties": {
			"enableRollback": {
			    "type": "boolean",
			    "default": false
			},
			"retryAfterRollback": {
			    "oneOf": [
				{
				    "type": "boolean"
				},
				{
				    "type": "number"
				}
			    ],
			    "default": false
			}
		    },
		    "default": {
			"enableRollback": false,
			"retryAfterRollback": false
		    }
		}
	    },
	    "default": {
		"target": "load",
		"findRelatedRecords": true,
		"resultsLevel": "total",
		"transaction": {
		    "enableRollback": false,
		    "retryAfterRollback": false
		}
	    }
	},
	"moduleParameters": {
	    "type": "object",
	    "properties": {
		"processors": {
		    "type": "object",
		    "properties": {
			"filter": {},
			"preprocess": {},
			"match": {},
			"merge": {},
			"load": {}
		    },
		    "additionalProperties": false,
		    "default": {}
		},
		"hooks": {
		    "type": "object",
		    "properties": {
			"beforeTransaction": {}
		    },
		    "additionalProperties": false,
		    "default": {}
		},
		"converter": {},
		"recordSet": {},
		"recordStore": {}
	    },
	    "default": {
		"processors": {}
	    }
	}
    }
}