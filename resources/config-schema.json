{
    "type": "object",
    "properties": {
	"logging": {
	    "type": "object",
	    "properties": {
		"level": {
		    "type": "string",
		    "enum": [
			"trace",
			"debug",
			"info",
			"warn",
			"error"
		    ]
		},
		"prefixes": {
		    "type": "array",
		    "items": [
			"timestamp",
			"level",
			"module"
		    ],
		    "default": ["timestamp", "level", "module"]
		}
	    },
	    "default": {
		"level": "warn",
		"prefixes": ["timestamp", "level", "module"]
	    }
	},
	"processing": {
	    "type": "object",
	    "properties": {
		"target": {
		    "type": "string",
		    "enum": [
			"filter",
			"preprocess",
			"match",
			"merge",
			"load"
		    ],
		    "default": "load"
		},
		"findRelatedRecords": {
		    "type": "boolean",
		    "default": true
		},
		"resultsLevel": {
		    "description": "Amount of data returned after processing is complete. With default level 'total' only total amounts are returned. Level 'record' returns also record-specific metadata about processing. Level 'debug' return the final representation at the target processing step of each record along with metadata",
		    "type": "string",
		    "enum": [
			"total",
			"record",
			"debug"
		    ],
		    "default": "total"
		},
		"maxWorkers": {
		    "type": "number"
		},
		"transaction": {
		    "type": "object",
		    "properties": {
			"enableRollback": {
			    "type": "boolean",
			    "default": false
			},
			"retryAfterRollback": {
			    "oneOf": [
				{
				    "type": "boolean"
				},
				{
				    "type": "number"
				}
			    ],
			    "default": false
			},
			"wait": {
			    "anyOf": [
				{
				    "type": "boolean"
				},
				{
				    "type": "array",
				    "items": {
					"enum": [
					    "preprocess",
					    "match",
					    "merge",
					    "load"
					]
				    }
				}
			    ],
			    "default": false
			},
			"async": {
			    "anyOf": [
				{
				    "type": "boolean"
				},
				{
				    "type": "object",
				    "properties": {
					"read": {
					    "type": "boolean"
					},
					"write": {
					    "type": "boolean"
					}
				    }
				}
			    ],
			    "default": false
			}
		    },
		    "default": {
			"enableRollback": false,
			"retryAfterRollback": false
		    }
		}
	    },
	    "default": {
		"target": "load",
		"findRelatedRecords": true,
		"resultsLevel": "total",
		"transaction": {
		    "enableRollback": false,
		    "retryAfterRollback": false
		}
	    }
	},
	"moduleParameters": {
	    "type": "object",
	    "properties": {
		"processors": {
		    "type": "object",
		    "properties": {
			"filter": {},
			"preprocess": {},
			"match": {},
			"merge": {},
			"load": {}
		    },
		    "additionalProperties": false,
		    "default": {}
		},
		"hooks": {
		    "type": "object",
		    "properties": {
			"beforeTransaction": {},
			"afterWait": {
			    "preprocess": {},
			    "match": {},
			    "merge": {},
			    "load": {}
			}
		    },
		    "additionalProperties": false,
		    "default": {}
		},
		"loggerFactory": {},
		"resultFormatter": {},
		"converter": {},
		"recordSet": {},
		"recordStore": {}
	    },
	    "default": {
		"processors": {},
		"hooks": {}
	    }
	}
    }
}